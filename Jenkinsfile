pipeline  
 {
    agent any


    stages
	{
        stage('Devops Project') 
		{
		
            steps 
			{
                echo 'Devops Project'
            }
        }
		 stage('Developer write the code') 
		{
		
            steps 
			{
                echo 'Developer write the code '
            }
        }
		 stage('push into the github') 
		{
		
            steps 
			{
                echo ' push into the github'
            }
        }
		
		stage('Testing the code') 
		{
		
            steps 
			{
                echo 'Testing the code'
            }
        }
		
    }
}

